import java.util.*;
class pract
{
public static void main(String args[])
{
int n,re,rev;
Scanner sc=new Scanner(System.in);
System.out.print("enter the number=");
n=sc.nextInt();
if(!sc.hasNextInt)
{
System.out.print("enter positive integrs only");
}
if(n<0)
{
System.out.print("invalid")
}
while(n!=0)
{
re=n%10;
rev=rev*10+re;
n=n/10;
System.out.print("  "+rev); 
}
}
} 
